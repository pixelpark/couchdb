{
  "name": "couchdb",
  "version": "1.1.0",
  "author": "Alexander Jakobsen, Mats Ove Mandt Skjaerstein",
  "summary": "Installs and configures CouchDB.",
  "license": "Apache-2.0",
  "source": "https://github.com/isberg1/couchdb",
  "dependencies": [
    { "name": "puppetlabs/stdlib",  "version_requirement": ">= 6.0.0 < 7.0.0" },
    { "name": "puppetlabs/apt",     "version_requirement": ">= 6.1.1 < 8.0.0" },
    { "name": "puppetlabs/inifile", "version_requirement": ">= 2.3.0 < 4.0.0" }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
