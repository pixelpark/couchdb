    { "operatingsystem": "OracleLinux", "operatingsystemrelease": [ "7" ] },
    { "operatingsystem": "Ubuntu",      "operatingsystemrelease": [ "18.04" ] }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 5.0.0 < 7.0.0" }
  ],
  "pdk-version": "1.7.1",
  "template-url": "file:///opt/puppetlabs/pdk/share/cache/pdk-templates.git",
  "template-ref": "1.7.1-0-g810b982"
